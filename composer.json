{
  "name": "lesname/value-object",
  "license": "MIT",
  "require": {
    "php": "^8.4.0",

    "ext-json": "*",
    "ext-intl": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^4.0.0",
    "phpunit/phpunit": "^12.0.0",

    "vimeo/psalm": "^7.0.0-beta9",

    "phpstan/phpstan": "^2.0.0",
    "phpstan/extension-installer": "^1.4.0",
    "phpstan/phpstan-deprecation-rules": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "LesValueObject\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LesValueObjectTest\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
